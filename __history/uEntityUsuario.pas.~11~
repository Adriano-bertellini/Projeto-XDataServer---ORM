unit uEntityUsuario;

interface

type
  [Entity, AutoMapping]
  [TABLE('USUARIOS')]
  TUsuario = class
  private
    [Id('FId', TIdGenerator.IdentityOrSequence)]
    FID: Integer;
    FNOME: string;
    FSENHA: string;

  public
    property ID: Integer read FID write FID;
    property NOME: string read FNOME write FNOME;
    property SENHA: string read FSENHA write FSENHA;

  end;

implementation

end.
