unit uEntityUsuario;

interface

type
  [Entity]
  [Sequence(GEN_USUARIOS_ID)]
  TUsuario = class
  private
    FID: Integer;
    FNOME: string;
    FSENHA: string;

  public
    property ID: Integer read FID write FID;
    property NOME: string read FNOME write FNOME;
    property SENHA: string read FSENHA write FSENHA;

  end;

implementation

end.
